# Example of what our .env looks like, actual in backend/
# Project name 'resite_project' is a placeholder, actual name in backend/

# Firebase Configuration
VITE_FIREBASE_API_KEY=our_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=resite_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=resite_project_id
VITE_FIREBASE_STORAGE_BUCKET=resite_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=our_sender_id
VITE_FIREBASE_APP_ID=our_app_id

# Supabase Configuration
VITE_SUPABASE_URL=https://resite_project.supabase.co
VITE_SUPABASE_ANON_KEY=our_supabase_anon_key

# Google Maps API
VITE_GOOGLE_MAPS_API_KEY=our_maps_api_key

# Google Cloud (for Supabase Edge Functions)
GOOGLE_CLOUD_PROJECT_ID=our_gcp_project_id
VERTEX_AI_LOCATION=us-central1